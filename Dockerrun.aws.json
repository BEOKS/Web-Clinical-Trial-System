{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": true,
            "image": "lee01042000/client-with-proxy:0.1",
            "name": "client-with-proxy",
            "hostname": "client-with-proxy",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "links":["middleware"],
            "memory": 300
        },
        {
            "essential": true,
            "image": "lee01042000/middleware:0.1",
            "hostname":"middleware",
            "mountPoints": [
                {
                    "containerPath": "/root/Storage",
                    "sourceVolume": "storage"
                }
            ],
            "name": "middleware",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "links":["mysql"],
            "memory": 300
        },
        {
            "command": [
                "--default-authentication-plugin=mysql_native_password"
            ],
            "environment": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "0000"
                }
            ],
            "essential": true,
            "image": "mysql:8.0-oracle",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/mysql",
                    "sourceVolume": "mysql"
                }
            ],
            "name": "mysql",
            "hostname":"mysql",
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 3306
                },
                {
                    "containerPort": 33060,
                    "hostPort": 33060
                }
            ],
            "memory": 300
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/app/current/Storage"
            },
            "name": "storage"
        },
        {
            "host": {
                "sourcePath": "/var/app/current/mysql"
            },
            "name": "mysql"
        }
    ]
}