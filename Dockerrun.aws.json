{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "lee01042000/nginx:0.1",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "backend"],
      "memory": 100
    },
    {
      "name": "client",
      "image": "lee01042000/client:0.1",
      "hostname": "client",
      "essential": false,
      "memory": 300
    },
    {
      "name": "backend",
      "image": "lee01042000/backend:0.1",
      "hostname": "backend",
      "essential": false,
      "links":["mysql"],
      "memory": 300
    },
    {
      "name": "mysql",
      "image": "mysql:8.0-oracle",
      "hostname": "mysql",
      "essential": false,
      "memory": 300
    }
  ]
}
